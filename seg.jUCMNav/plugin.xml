<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

     <extension-point id="UseCaseMapExport" name="%UseCaseMapExport" schema="schema/UseCaseMapExport.exsd"/>
   <extension-point id="URNExport" name="%UrnExport" schema="schema/URNExport.exsd"/>
   <extension-point id="URNImport" name="%UrnImport" schema="schema/URNImport.exsd"/>
   <extension-point id="GRLStrategyAlgorithm" name="%GrlStrategyAlgorithm" schema="schema/GRLStrategyAlgorithm.exsd"/>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="seg.jUCMNav.editors.UCMNavMultiPageEditor"
            contributorClass="seg.jUCMNav.editors.actionContributors.UCMActionBarContributor"
            default="true"
            extensions="jucm"
            icon="icons/icon16.gif"
            id="seg.jUCMNav.MainEditor"
            name="seg.jUCMNav.MainEditor"/>
   </extension>
   <extension
         point="org.eclipse.ui.editorActions">
      <editorContribution
            targetID="seg.jUCMNav.MainEditor"
            id="seg.jUCMNav.editorContribution1">
         <action
               toolbarPath="jUCMNav"
               label="%autoLayout"
               class="seg.jUCMNav.actions.AutoLayoutActionDelegate"
               tooltip="%autoLayout"
               icon="icons/Node16.gif"
               style="push"
               menubarPath="seg.jUCMNav/ext"
               id="seg.jUCMNav.AutoLayout"
               actionID="seg.jUCMNav.actions.AutoLayoutActionDelegate">
         </action>
         <menu
               label="jUCMNav"
               id="seg.jUCMNav">
            <separator name="ext"/>
         </menu>
         <action
               actionID="seg.jUCMNav.actions.AddMapAction"
               class="seg.jUCMNav.actions.UCMActionDelegate"
               icon="icons/icon16.gif"
               id="seg.jUCMNav.AddMap"
               label="%AddUseCaseMap"
               menubarPath="seg.jUCMnav/ext"
               style="push"
               tooltip="%AddUseCaseMap"/>
         <action
               actionID="seg.jUCMNav.actions.AddGrlGraphAction"
               class="seg.jUCMNav.actions.UCMActionDelegate"
               icon="icons/icon16.gif"
               id="seg.jUCMNav.AddGrlGraph"
               label="%AddGrlDiagram"
               menubarPath="seg.jUCMnav/ext"
               style="push"
               tooltip="%AddGrlDiagram"/>
      </editorContribution>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="seg.jUCMNav.views.UCMPerspectiveFactory"
            icon="icons/icon16.gif"
            id="seg.jUCMNav.UCMEditor"
            name="jUCMNav"/>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="jUCMNav"
            id="seg.jUCMNav"/>
      <wizard
            category="seg.jUCMNav"
            class="seg.jUCMNav.views.wizards.NewUcmFileWizard"
            descriptionImage="icons/perspectiveIcon.gif"
            finalPerspective="seg.jUCMNav.UCMEditor"
            icon="icons/icon16.gif"
            id="seg.jUCMNav.views.wizards.NewUcmFileWizard"
            name="%UseCaseMapGrlGraph"
            preferredPerspectives="seg.jUCMNav.UCMEditor"
            project="true"/>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension targetID="seg.jUCMNav.UCMEditor">
         <viewShortcut id="seg.jUCMNav.views.ElementView"/>
         <viewShortcut id="seg.jUCMNav.views.StrategiesView"/>
         <newWizardShortcut id="seg.jUCMNav.views.wizards.NewUcmFileWizard"/>
         <viewShortcut id="org.eclipse.ui.views.ResourceNavigator"/>
         <viewShortcut id="org.eclipse.ui.views.ContentOutline"/>
         <viewShortcut id="org.eclipse.ui.views.PropertySheet"/>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="jUCMNav"
            id="seg.jUCMNav"/>
      <view
            class="seg.jUCMNav.views.elements.ElementView"
            icon="icons/Resp16.gif"
            category="seg.jUCMNav"
            name="%Elements"
            id="seg.jUCMNav.views.ElementView"/>
      <view
            category="seg.jUCMNav"
            class="seg.jUCMNav.views.strategies.StrategiesView"
            icon="icons/StategyView16.gif"
            id="seg.jUCMNav.views.StrategiesView"
            name="%Strategies"/>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc file="help.xml" primary="true"/>
   </extension>
   <extension
         point="org.eclipse.ui.exportWizards">
      <wizard
            icon="icons/perspectiveIcon.gif"
            class="seg.jUCMNav.views.wizards.importexport.ExportWizard"
            name="%exportToImage"
            id="seg.jUCMNav.views.wizards.importexport.ExportWizard">
         <selection
               class="org.eclipse.core.resources.IFile"
               name="*.jucm"/>
      </wizard>
   </extension>
   <extension
         id="seg.jUCMNav.importexport.ExportImage"
         name="%exportToImage"
         point="seg.jUCMNav.UseCaseMapExport">
      <exporter
            class="seg.jUCMNav.importexport.ExportLayoutDOT"
            extension="dot"
            id="seg.jUCMNav.ExportDOT"
            mode="model instance"
            name="%ExportDOT"
            useStream="true"/>
      <exporter
            class="seg.jUCMNav.importexport.ExportImageJPG"
            extension="jpg"
            id="seg.jUCMNav.ExportJPG"
            mode="image"
            name="%ExportJPG"
            useStream="true"/>
      <exporter
            class="seg.jUCMNav.importexport.ExportImageBMP"
            extension="bmp"
            id="seg.jUCMNav.ExportBMP"
            mode="image"
            name="%ExportBMP"
            useStream="true"/>
      <exporter
            class="seg.jUCMNav.importexport.ExportImageGIF"
            extension="gif"
            id="seg.jUCMNav.ExportGIF"
            mode="image"
            name="%ExportGIF"
            useStream="true"/>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="seg.jUCMNav.views.preferences.GeneralPreferencePage"
            class="seg.jUCMNav.views.preferences.AutoLayoutPreferencePage"
            id="seg.jUCMNav.views.preferences.AutoLayoutPreferencePage"
            name="%AutolayoutPreferences"/>
      <page
            class="seg.jUCMNav.views.preferences.GeneralPreferencePage"
            id="seg.jUCMNav.views.preferences.GeneralPreferencePage"
            name="%jUCMNavPreferences"/>
      <page
            category="seg.jUCMNav.views.preferences.GeneralPreferencePage"
            class="seg.jUCMNav.views.preferences.ScenarioTraversalPreferencePage"
            id="seg.jUCMNav.views.preferences.ScenarioTraversalPreferencePage"
            name="UCM Scenario Traversal Algorithm"/>
      <page
            category="seg.jUCMNav.views.preferences.GeneralPreferencePage"
            class="seg.jUCMNav.views.preferences.StrategyEvaluationPreferencePage"
            id="seg.jUCMNav.views.preferences.StrategyEvaluationPreferencePage"
            name="GRL Strategy Evaluation Algorithm"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="seg.jUCMNav.views.preferences.PreferenceInitializer"/>
   </extension>
   <extension
         id="seg.jUCMNav.importexport.ExportURN"
         name="%exportToImage"
         point="seg.jUCMNav.URNExport">
      <exporter
            class="seg.jUCMNav.importexport.ExportDXL"
            extension="dxl"
            id="seg.jUCMNav.ExportDXL"
            name="%ExportDXL"
            postExporter="seg.jUCMNav.ExportBMP"
            useStream="false"/>
      <exporter
            class="seg.jUCMNav.importexport.ExportGRLCatalog"
            extension="grl"
            id="seg.jUCMNav.ExportGRLCatalog"
            name="%GRLCatalog"
            useStream="false"/>
      <exporter
            class="seg.jUCMNav.importexport.ExportCSV"
            extension="csv"
            id="seg.jUCMNav.ExportCSV"
            name="%ExportCSV"
            useStream="false"/>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            class="seg.jUCMNav.views.wizards.importexport.ImportWizard"
            icon="icons/perspectiveIcon.gif"
            id="seg.jUCMNav.views.wizards.importexport.ImportWizard"
            name="%import"/>
   </extension>
   <extension
         id="seg.jUCMNav.importexport.ImportURN"
         name="%import"
         point="seg.jUCMNav.URNImport">
      <importer
            class="seg.jUCMNav.importexport.ImportGRLCatalog"
            extension="grl"
            id="seg.jUCMNav.ImportGRLCatalog"
            importInSelectedFile="true"
            name="%GRLCatalog"
            useStream="true"/>
   </extension>
   <extension
         id="seg.jUCMNav.MarkerSupport"
         name="jUCMNav Marker Support"
         point="org.eclipse.ui.ide.markerSupport">
      <markerTypeCategory name="seg.jUCMNav.WarningMarker"/>
      <problemFilter
            enabled="true"
            id="seg.jUCMNav.problemFilter1"
            name="seg.jUCMNav.problemFilter1"
            scope="ON_SELECTED_ONLY"/>
   </extension>
   <extension
         id="jUCMNav.MarkerResolution"
         name="jUCMNav Quick Fix"
         point="org.eclipse.ui.ide.markerResolution">
      <markerResolutionGenerator
            class="seg.jUCMNav.views.QuickFixer"
            markerType="org.eclipse.core.resources.problemmarker"/>
   </extension>

</plugin>
