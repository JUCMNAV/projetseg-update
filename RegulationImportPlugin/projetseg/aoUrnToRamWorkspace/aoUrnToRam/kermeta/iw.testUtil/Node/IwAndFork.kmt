package intermediateWorkflow;

require "platform:/lookup/aoUrnToRam/kermeta/iw.testUtil/_Ref.kmt"

using kermeta::standard

aspect class IwAndFork
{
	operation withWorkflow(workflow:IwWorkflow):IwAndFork is do
		self.workflow:=workflow
		result:=self
	end
	
	operation withStep(step:IwStep):IwAndFork is do
		self.step:=step
		result:=self
	end
}