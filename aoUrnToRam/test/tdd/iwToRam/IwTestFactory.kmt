package aoUrnToRam::Test;

require kermeta
require "http:///intermediateWorkflow.ecore"

using kermeta::standard
using kermeta::kunit
using intermediateWorkflow

class IwTestFactory
{
	operation createIwModel():IwModel is do
		result:=IwModel.new
	end

	operation createIwWorkflow():IwWorkflow is do
		result:=IwWorkflow.new
	end

	operation createIwInput():IwInput is do
		var res:IwInput init IwInput.new
		res.name:="a"
		result:=res
	end
	
	operation createIwProcessingNode():IwProcessingNode is do
		var res:IwProcessingNode init IwProcessingNode.new
		res.name:="a"
		result:=res
	end
	
	operation createIwOutput():IwOutput is do
		var res:IwOutput init IwOutput.new
		res.name:="a"
		result:=res
	end
	
	operation createIwEndPoint():IwEndPoint is do
		result:= IwEndPoint.new
		result.name:="a"
	end
	
	operation createIwNodeConnection(source:IwNode,target:IwNode):IwNodeConnection is do
		result:=IwNodeConnection.new
		result.source:=source
		result.target:=target
	end
}