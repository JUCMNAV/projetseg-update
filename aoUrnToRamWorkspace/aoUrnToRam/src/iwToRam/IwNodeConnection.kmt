package intermediateWorkflow;


require "platform:/resource/aoUrnToRam/src/iwToRam/_Ref.kmt"

using kermeta::standard
using ramstructural

aspect class IwNodeConnection {
	reference ramCondition:Class
	
	operation build() is do
		ramCondition:=Class.new
		ramCondition.name:=conditionNameWithSourcePrefix
	end
	
	operation link() is do
		step.addMappedRamClass(step.REACTIVE_WORKFLOW_CONDITION,ramCondition)
	end
}