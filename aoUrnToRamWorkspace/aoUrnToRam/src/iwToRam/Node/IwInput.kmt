package intermediateWorkflow;

require "platform:/resource/aoUrnToRam/src/iwToRam/_Ref.kmt"

using kermeta::standard
using ramstructural

aspect class IwInput{
	reference ramInputData : Class
	
	method build() is do
		ramInputData:=createRamClass(name+INPUT_DATA_SUFFIX)
	end
	
	method link() is do
		step.addMappedRamClass(step.REACTIVE_WORKFLOW_INPUT_DATA,ramInputData)
	end
}