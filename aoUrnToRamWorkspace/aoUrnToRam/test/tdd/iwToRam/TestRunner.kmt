require "platform:/resource/aoUrnToRam/test/tdd/iwToRam/IwStepTestCase.kmt"
require "platform:/resource/aoUrnToRam/test/tdd/iwToRam/IwModelTestCase.kmt"

require "platform:/resource/aoUrnToRam/test/CustomTestRunner.kmt"

using kermeta::standard
using kermeta::language::structure

class IwToRamTestRunner inherits CustomTestRunner
{
	method getTestClasses(): OrderedSet<Class> is do
		result:=OrderedSet<Class>.new
		result.add(IwToRam_IwNodeConnectionTestCase)
		result.add(IwToRam_IwOrForkTestCase)
		result.add(IwToRam_IwStep_CustomizableNodeTestCase)
		result.add(IwToRam_IwStepTestCase)
		result.add(IwToRam_IwModelTestCase)
	end
}

