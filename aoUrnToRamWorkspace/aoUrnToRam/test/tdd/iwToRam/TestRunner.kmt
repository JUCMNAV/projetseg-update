@mainClass "aoUrnToRam::Test::IwToRamTestRunner"
@mainOperation "runTestClassesWithReport"
package aoUrnToRam::Test;

require kermeta
require "platform:/resource/aoUrnToRam/test/tdd/iwToRam/IwNodeConnectionTestCase.kmt"
require "platform:/resource/aoUrnToRam/test/tdd/iwToRam/Node/IwOrForkTestCase.kmt"
