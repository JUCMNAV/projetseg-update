@mainClass "aoUrnToRam::test::AllTestRunner"
@mainOperation "runTestClassesWithReport"
package aoUrnToRam::test;

require kermeta
require "platform:/resource/aoUrnToRam/test/tdd/iwToIw/InsertInputProcessingNodes/TestRunner.CodeGen.kmt"
require "platform:/resource/aoUrnToRam/test/tdd/iwToIw/LinkSteps/TestRunner.CodeGen.kmt"
require "platform:/resource/aoUrnToRam/test/tdd/iwToRam/TestRunner.CodeGen.kmt"
require "platform:/resource/aoUrnToRam/test/CustomTestRunner.kmt"

using kermeta::standard
using kermeta::language::structure

class AllTestRunner inherits CustomTestRunner
{
	method getTestClasses(): OrderedSet<Class> is do
		result:=OrderedSet<Class>.new
		result.addAll(InsertInputProcessingNodes_TestRunner.new.getTestClasses)
		result.addAll(LinkSteps_TestRunner.new.getTestClasses)
		result.addAll(IwToRam_TestRunner.new.getTestClasses)
	end
}