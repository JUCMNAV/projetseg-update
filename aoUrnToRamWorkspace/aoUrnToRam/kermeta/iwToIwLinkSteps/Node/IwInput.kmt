package intermediateWorkflow;

require "platform:/lookup/aoUrnToRam/kermeta/iwToIwLinkSteps/_Ref.kmt"

using kermeta::standard

aspect class IwInput {
	method explore(currentStep:IwStep) is do
		if(currentStep.isStartupStep==true) then
			linkStartupStep(currentStep)
		else
			currentStep:=createStep(false)
		end
		super(currentStep)
	end
	
	operation linkStartupStep(currentStep:IwStep) is do
		currentStep.name:=name
		currentStep.isStartupStep:=false
	end
	
}