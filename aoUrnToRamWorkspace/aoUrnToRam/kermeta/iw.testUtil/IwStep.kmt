using intermediateWorkflow

aspect class IwStep
{
	operation withName(name:String):IwStep is do
		self.name:=name
		result:=self
	end

	operation withConcern(concern:IwConcern):IwStep is do
		self.concern:=concern
		result:=self
	end
}